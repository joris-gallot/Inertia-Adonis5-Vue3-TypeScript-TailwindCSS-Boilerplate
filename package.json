{
  "name": "inertia-adonis5-vue3-typescript",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.6.2",
    "@japa/preset-adonis": "^1.0.15",
    "@japa/runner": "^2.0.7",
    "adonis-preset-ts": "^2.1.0",
    "autoprefixer": "^10.4.5",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "laravel-mix": "^6.0.43",
    "pino-pretty": "^7.6.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "vue-loader": "^16.8.3",
    "youch": "^3.1.1",
    "youch-terminal": "^2.1.3"
  },
  "dependencies": {
    "@adonisjs/core": "^5.7.5",
    "@adonisjs/repl": "^3.1.10",
    "@adonisjs/session": "^6.2.2",
    "@adonisjs/shield": "^7.0.10",
    "@adonisjs/view": "^6.1.4",
    "@eidellev/inertia-adonisjs": "^6.2.0",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@inertiajs/inertia": "^0.11.0",
    "@inertiajs/inertia-vue3": "^0.6.0",
    "@vue/compiler-sfc": "^3.2.33",
    "adonis-mix-asset": "^3.0.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "vue": "^3.2.33",
    "vue-template-compiler": "^2.6.14"
  }
}
